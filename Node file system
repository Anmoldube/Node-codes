const fs = require("fs");

//scy
// fs.writeFileSync("./text.txt", "hello .");

//Async
fs.writeFile("./tex.txt", "hello it's me anmgsdgsol...", (err) => {});

fs.readFile("./tex.txt", "utf-8", (err, data) => {
  if (err) {
    console.log(err);
  } else {
    console.log(data);
  }
});


