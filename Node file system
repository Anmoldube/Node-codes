const math = require("./maths");
console.log(
  "maths function Ans",
  math.add(1, 2) + " maths function sub:",
  math.sub(1, 2)
);


function add(a, b) {
  return a + b;
}
function sub(a, b) {
  return a - b;
}
module.exports = {
  add,
  sub,
};


